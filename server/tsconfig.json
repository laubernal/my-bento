{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": false,
    "target": "es2017",
    "esModuleInterop": true,
    "incremental": true,
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": ".",
    "paths": {
      "Shared/*": [
        "./src/Shared/*"
      ],
      "Authorization/*": [
        "./src/Authorization/*",
        "./src/Authorization"
      ]
    },
  }
}